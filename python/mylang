tk :: import("tkinter");
ttk :: import("tkinter.ttk");
msg :: import("tkinter.message");


onclick :: () {
  msg.showinfo("A program written with an imagenery language."); 
}

main :: () {
    root := tk.Tk();
    root.title("Hello World Program");
    button = ttk.Button(root, text="Some button");
    button.config(command=onclick);
    button.pack();
    root.mainloop();
}

